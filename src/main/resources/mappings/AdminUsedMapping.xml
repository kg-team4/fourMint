<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AdminUsedDAO">
	
	<resultMap  type="adminUsed" id="adminResult">
		<result property="market_seq" column="MARKET_SEQ" />
		<result property="category_big" column="CATEGORY_BIG" />
		<result property="category_middle" column="CATEGORY_MIDDLE" />	
		<result property="product_name" column="PRODUCT_NAME" />	
		<result property="product_price" column="PRODUCT_PRICE" />
	</resultMap>
	
	<insert id="insertAdmin" parameterType="string">
		INSERT INTO marketboard (market_seq, category_big, category_middle, product_name, product_price) 
		VALUES (#{market_seq}, #{category_big}, #{category_middle}, #{product_name}, #{product_price})
	</insert>
	
	<select id="getAdminUsedList" resultType="adminUsed" parameterType="string">
		SELECT * FROM marketboard
	</select>	
	
	<delete id="delete" parameterType="int">
		delete from marketboard where market_seq = #{market_seq}
	</delete>	
</mapper>